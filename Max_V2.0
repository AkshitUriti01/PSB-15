// Online C compiler to run C program online
#include <stdio.h>

int main() {
     int a,b,c;
    printf("Enter first number: ");
    scanf("%i", &a);
    
    printf("Enter second number: ");
    scanf("%i", &b);
    
    printf("Enter third number: ");
    scanf("%i", &c);
    
    if (a>=b && a>=c) {
        printf("%d is largest.", a);
    } else if (b>=a && b>=c) {
        printf("%d is largest.", b);
    } else if (c>=a && c>=b) {
        printf("%d is the largest.", c);
    } 
    
    return 0;
}
